{
    "manifest_version": 2,

    "name": "Cowin Booking",
    "description": "automatically fill data on cowin portal to speed up covid vaccine slot booking",
    "version": "2.0.5",
    "content_scripts": [
        {
            "matches": ["https://selfregistration.cowin.gov.in/*"],
            "js": [
                "jquery.min.js",
                "contentScript.js",
                "shortcut.js"
            ]
        }
    ],
    "icons": {
        "128": "icon.png"
    }
}
